<div class="container mx-auto">
  <% if @projects.any? %>
    <div class="grid md:grid-cols-3 sm:grid-cols-2">
      <% @projects.each do |project| %>
        <%= render "projects/project", project: project %>
      <% end %>
    </div>
  <% else %>
    <p>There isn't any projects yet.</p>
  <%end %>
</div>